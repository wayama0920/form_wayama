<web-app>
	<servlet>
		<servlet-name>loginMapping</servlet-name>
		<servlet-class>servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginMapping</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>formMapping</servlet-name>
		<servlet-class>servlet.FormServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>formMapping</servlet-name>
		<url-pattern>/form</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>inquiryListMapping</servlet-name>
		<servlet-class>servlet.InquiryListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>inquiryListMapping</servlet-name>
		<url-pattern>/inquiryList</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>inquiryContentsMapping</servlet-name>
		<servlet-class>servlet.InquiryContentsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>inquiryContentsMapping</servlet-name>
		<url-pattern>/inquiryContents</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>logoutMapping</servlet-name>
		<servlet-class>servlet.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logoutMapping</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>inquiryReplyMapping</servlet-name>
		<servlet-class>servlet.InquiryReplyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>inquiryReplyMapping</servlet-name>
		<url-pattern>/inquiryReply</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>inquiryEmailMapping</servlet-name>
		<servlet-class>servlet.InquiryEmailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>inquiryEmailMapping</servlet-name>
		<url-pattern>/inquiryEmail</url-pattern>
	</servlet-mapping>

</web-app>